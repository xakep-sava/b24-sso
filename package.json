    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-inlinesvg": "^1.2.0",
    "react-jss": "^10.1.1",
